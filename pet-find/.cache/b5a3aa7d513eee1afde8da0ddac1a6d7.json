) {\r\n  console.log(data);\r\n}\r\n\r\n// Show Listing of Pets\r\nfunction showAnimals(pets) {\r\n  const results = document.querySelector(\"#results\");\r\n  // Clear first\r\n  results.innerHTML = \"\";\r\n  // Loop Through Pets\r\n  pets.forEach(pet => {\r\n    console.log(pet);\r\n    const div = document.createElement(\"div\");\r\n    div.classList.add(\"card\", \"card-body\", \"mb-3\");\r\n    div.innerHTML = `\r\n        <div class=\"row\">\r\n            <div class=\"col-sm-6\">\r\n                <h4>${pet.name.$t} (${pet.age.$t})</h4>\r\n                <p class=\"text-secondary\">${pet.breeds.breed.$t}</p>\r\n                <p>\r\n                ${pet.contact.address1.$t} ${pet.contact.city.$t}\r\n                ${pet.contact.state.$t} ${pet.contact.zip.$t}\r\n                </p>\r\n                <ul class=\"list-group\">\r\n                    <li class=\"list-group-item\">Phone: ${\r\n                      pet.contact.phone.$t\r\n                    }</li>\r\n                    ${\r\n                      pet.contact.email.$t\r\n                        ? `<li class=\"list-group-item\">Email: ${\r\n                            pet.contact.email.$t\r\n                          }</li>`\r\n                        : ``\r\n                    }\r\n                    <li class=\"list-group-item\">Shelter ID: ${\r\n                      pet.shelterId.$t\r\n                    }</li>\r\n                </ul>\r\n            </div>\r\n            <div class=\"col-sm-6 text-center\">\r\n                  <img class=\"img-fluid rounded-circle mt-2\" src=\"${\r\n                    pet.media.photos.photo[3].$t\r\n                  }\">  \r\n            </div>\r\n        </div>\r\n    `;\r\n\r\n    results.appendChild(div);\r\n  });\r\n}\r\n"},"lineCount":null}},"hash":"5562b3d0adb0ef8da6f3ba28320925d1","cacheData":{"env":{}}}